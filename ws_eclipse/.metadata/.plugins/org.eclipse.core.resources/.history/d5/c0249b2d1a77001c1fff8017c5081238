import java.util.Scanner;

public class MainTestScanner {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		String x;
		x = sc.next();
		System.out.println("Você digitou: " + x);
		
		int y;
		y = sc.nextInt();
		System.out.println(x + "digitou: " + y);

		sc.close();
	}

}
